{
  "name": "clone-comparison-nodejs",
  "version": "1.0.0",
  "description": "A comparison of different deep cloning libraries for use in Node.js",
  "scripts": {
    "build": "yarn run clean && yarn run tsc && yarn run tslint && yarn run test",
    "clean": "rimraf ./dist",
    "test": "mocha -r source-map-support/register ./dist",
    "test-report": "mocha -r source-map-support/register --reporter dist/src/comparisonReporter.js ./dist",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "tslint": "tslint --project tsconfig.json"
  },
  "author": "Laurence Dougal Myers",
  "repository": "https://github.com/laurence-myers/clone-comparison-nodejs",
  "license": "ISC",
  "private": true,
  "engines": {
    "node": ">=8.9.4"
  },
  "dependencies": {
    "@types/mocha": "2.2.48",
    "@types/node": "9.4.6",
    "clone": "2.1.1",
    "clone-deep": "2.0.1",
    "clone-extend": "0.1.2",
    "component-clone": "0.2.3",
    "deep-clone": "3.0.2",
    "deep-copy": "1.4.2",
    "deepclone": "1.0.2",
    "deepcopy": "0.6.3",
    "es6-deep-clone": "1.1.1",
    "extend": "3.0.1",
    "fast-clone": "1.4.2",
    "fast-deepclone": "1.0.0",
    "fs-extra": "5.0.0",
    "he": "1.1.1",
    "lodash.clonedeep": "4.5.0",
    "mocha": "4.0.1",
    "outdent": "0.4.1",
    "source-map-support": "0.5.3",
    "stringify-clone": "2.0.0",
    "structured-clone": "0.2.2",
    "utils-copy": "1.1.1"
  },
  "devDependencies": {
    "@types/fs-extra": "5.0.0",
    "@types/he": "0.5.29",
    "rimraf": "2.6.2",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  }
}
