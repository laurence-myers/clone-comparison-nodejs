    "clean": "rimraf ./dist",
    "test": "mocha -r source-map-support/register ./dist",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "tslint": "tslint --project tsconfig.json"
  },
  "author": "Laurence Dougal Myers",
  "license": "ISC",
  "engines": {
    "node": ">=8.9.4"
  },
  "dependencies": {
    "@types/mocha": "2.2.48",
    "@types/node": "9.4.6",
    "clone": "2.1.1",
    "clone-deep": "2.0.1",
    "clone-extend": "0.1.2",
    "component-clone": "0.2.3",
    "deep-clone": "3.0.2",
    "fast-deepclone": "1.0.0",
    "mocha": "4.0.1",
    "source-map-support": "0.5.3"
  },
  "devDependencies": {
    "rimraf": "2.6.2",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  }
